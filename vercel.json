{
  "version": 2,
  "builds": [
    { "src": "/api/**/*", "use": "@vercel/node" },
    { "src": "/public/**/*", "use": "@vercel/php" }
  ],
  "env": {
    "LD_LIBRARY_PATH": "/usr/lib/x86_64-linux-gnu:/usr/local/lib/x86_64-linux-gnu:/lib/x86_64-linux-gnu:/usr/lib:/lib:/vercel/path0/.vercel/nodeserver/.vercel/node12/lib"
  }
}
